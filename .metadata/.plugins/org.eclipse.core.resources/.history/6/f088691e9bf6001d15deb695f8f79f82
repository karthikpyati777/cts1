package com.java.collections;

import java.util.ArrayList;
import java.util.List;

public class NeedOfCollection {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		List<Integer> l=new ArrayList();
		l.add(1);
		l.add(2);
		l.add(4);
		l.add(7);
		l.add(9);
		l.add(13);
		for(int i:l) {
			System.out.print(i+" ");
		}

	}

}
