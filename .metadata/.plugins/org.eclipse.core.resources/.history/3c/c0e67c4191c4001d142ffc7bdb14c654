package com.basic.javapgms;

public class P2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int n=5;
		for(int i=1;i<n;i++) {
			for(int j=1;j<i+1;j++) {
				System.out.print("*");
			}
			System.out.println();
			
		
		for(int k=n-1;k>=1;k--) {
			for(int j=1;j<k-1;j++) {
				System.out.print("*");
			}
			System.out.println();
		}

		
		}
	}

}
